<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#33458c" />
            <!--gradient android:angle="-90" android:startColor="#345953" android:endColor="#689a92"  /-->
            <gradient android:angle="-90" android:startColor="#232f60" android:endColor="#232f60"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#33458c" />
            <solid android:color="#33458c"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#33458c" />
            <!--gradient android:angle="-90" android:startColor="#8dbab3" android:endColor="#58857e" /-->
            <gradient android:angle="-90" android:startColor="#33458c" android:endColor="#33458c" />
        </shape>
    </item>
</selector>